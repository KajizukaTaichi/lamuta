const Fraction: struct = @{
    "numerator": 1,
    "denominator": 1,
    "asText": fn(this ->
        (this.numerator as text) + "/" + (this.denominator as text)
    ),
    "asNum": fn(this ->
        this.numerator / this.denominator
    )
};

let hoge = Fraction + @{ "numerator": 2, "denominator": 5 };
print hoge.asText hoge, hoge.asNum hoge;
