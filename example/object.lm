const Fraction: #Fraction = @{
    "numerator": 0,
    "class": #Fraction,
    "denominator": 1,
    "asText": fn(this ->
        f"{this.numerator}/{this.denominator}"
    ),
    "asNumber": fn(this ->
        this.numerator / this.denominator
    )
};

let hoge = Fraction + @{ "numerator": 2, "denominator": 5 };
print hoge as text, " = ", hoge as number;
