load library.lm;

let fzbzResult = Γ"Fizz"+"Buzz"+"FizzBuzz";
let fzbzJudge = λn.{
    match [n % 3, n % 5] {
        [0, 0] → fzbzResult new "FizzBuzz",
        [0, _] → fzbzResult new "Fizz",
        [_, 0] → fzbzResult new "Buzz",
        [_, _] → n as Γtext
    }
};

let compose = λf.λg.λx.f(g(x));
let incFzbz = compose fzbzJudge λx.(x + 1);
print join (map incFzbz (range 100)) " "
